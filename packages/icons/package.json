{
  "name": "@foxone/icons",
  "version": "4.0.2",
  "description": "UIKit custom icons",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "generate": "rm -rf src && node scripts/build.js && yarn format",
    "format": "prettier --write src",
    "build": "babel src --out-dir lib --extensions \".ts\",\".tsx\" && tsc --pretty --emitDeclarationOnly",
    "dev": "yarn build --watch"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "license": "MIT",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.12",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
