{
  "name": "@foxone/uikit",
  "version": "4.0.4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "start-storybook -p 6006",
    "docs": "build-storybook",
    "build": "yarn build:lib && yarn build:types",
    "build:dist": "rm -rf dist && rollup --config config/rollup.config.mjs",
    "build:lib": "rm -rf lib && gulp --gulpfile config/gulp.config.js",
    "build:types": "tsc --pretty --emitDeclarationOnly -p tsconfig.lib.json && node config/gendts.js"
  },
  "dependencies": {
    "@foxone/utils": "^0.2.26",
    "qrcode": "^1.5.1",
    "tsconfig": "^7.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.40",
    "vuetify": "^3.0.0-beta.13"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./sass/main": "./src/styles/_main.scss",
    "./sass/setting": "./src/styles/_settings.scss",
    "./components": "./lib/components/index.js",
    "./components/*": "./lib/components/*/index.js",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "lib/index.js": [
        "lib/index.d.ts",
        "lib/shim.d.ts"
      ],
      "*": [
        "*",
        "lib/*",
        "lib/*.d.ts",
        "lib/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@foxone/icons": "*",
    "@foxone/utils": "^0.2.26",
    "@rollup/plugin-alias": "^4.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.12",
    "@storybook/vue3": "^6.5.12",
    "@typescript-eslint/utils": "^5.42.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "cssnano": "^5.1.13",
    "eslint-utils": "^3.0.0",
    "glob": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat-css": "^3.1.0",
    "gulp-copy": "^4.0.1",
    "gulp-header": "^2.0.9",
    "gulp-replace-path": "^0.4.0",
    "gulp-rollup": "^2.17.0",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "node-sass": "^6.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss": "^8.4.18",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-sass": "^1.2.12",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.55.0",
    "typescript": "^4.9.3",
    "vite": "^3.1.7",
    "vite-plugin-vuetify": "^1.0.0",
    "vue": "^3.2.40",
    "vue-loader": "^16.8.3",
    "vuetify": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
